{
  "name": "d365js",
  "version": "1.0.0",
  "description": "A starter kit for writing professional d365 JS webresources",
  "scripts": {
    "lint": "eslint --ext .jsx,.js src/",
    "test": "npx jest --verbose ./test",
    "doc": "jsdoc -c jsdoc.json",
    "//Form": "****************** -- source.js --o destination.js",
    "build:form:dev": "%npm_package_scripts_buildDevForm%",
    "build:form:prd": "%npm_package_scripts_buildPrdForm%",
    "//Form & Chunk": "****************** -- source.js --o destination.js",
    "build:chunk:dev": "%npm_package_scripts_buildDevChunk%",
    "build:chunk:prd": "%npm_package_scripts_buildPrdChunk%",
    "//NPM": "*****************************************************************",
    "buildDev": "npm run lint src && npx webpack --config webpack/config.dev.js",
    "buildDevChunk": "npm run lint src && npx webpack --config webpack/config.dev.chunk.js",
    "buildDevForm": "npm run lint src && npx webpack --config webpack/config.dev.form.js",
    "buildPrd": "npx webpack --config webpack/config.prd.js",
    "buildPrdChunk": "npx webpack --config webpack/config.prd.chunk.js",
    "buildPrdForm": "npx webpack --config webpack/config.prd.form.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "d365",
    "javascript",
    "webresources",
    "d365",
    "webresource"
  ],
  "author": "Rex Kenley Tan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.1",
    "@types/jest": "^24.0.25",
    "core-js": "^3.6.2",
    "react": "^16.14.0",
    "snyk": "^1.419.1",
    "taiko": "^1.0.25",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.6",
    "prettier": "1.19.1",
    "webpack-cli": "^3.3.12"
  },
  "snyk": true
}
